---
import Layout from "../layouts/Layout.astro";
---

<Layout title="404 - Andrew Ryno" description={"404 Not Found"} canonical="">
    <div class="max-w-lg mx-auto my-10 bg-white rounded-lg shadow-md p-5">
        <h2 class="text-center text-2xl font-semibold mt-3">404 Not Found</h2>
        <div class="mt-5">
            <p class="text-gray-600 mt-2">
                Sorry, you were looking for something that isn't here.
            </p>
        </div>
    </div>
    <div class="flex justify-center mt-5">
        <a
            href="https://www.linkedin.com/in/andrewryno/"
            class="text-blue-500 hover:text-blue-700 mx-3">LinkedIn</a
        >
        <a
            href="https://github.com/andrewryno"
            class="text-blue-500 hover:text-blue-700 mx-3">GitHub</a
        >
    </div>
</Layout>
